<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.housingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity"></activity>
        <activity android:name=".Activity.SignupActivity"></activity>
        <activity android:name=".Activity.FeatutresActivity"></activity>
        <activity android:name=".Activity.CallendarActivity$FeedbackActivity"></activity>
        <activity android:name=".Activity.CallendarActivity"></activity>
        <activity android:name=".Activity.RulesDisplayActivity"></activity>
        <activity android:name=".Activity.ProfileActivity"></activity>
        <activity android:name=".Activity.HelpActivity"></activity>
        <activity android:name=".Activity.MemberActivity"></activity>
    </application>

</manifest>